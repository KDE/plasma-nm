#, kde-format
msgid "Allow/disable authentication methods."
msgstr "Permet/desactiva els mètodes d'autenticació."

#. i18n: ectx: property (text), item, widget (QListWidget, listWidget)
#: pptpadvanced.ui:40
#, kde-format
msgid "PAP"
msgstr "PAP"

#. i18n: ectx: property (text), item, widget (QListWidget, listWidget)
#: pptpadvanced.ui:48
#, kde-format
msgid "CHAP"
msgstr "CHAP"

#. i18n: ectx: property (text), item, widget (QListWidget, listWidget)
#: pptpadvanced.ui:56
#, kde-format
msgid "MSCHAP"
msgstr "MSCHAP"

#. i18n: ectx: property (text), item, widget (QListWidget, listWidget)
#: pptpadvanced.ui:64
#, kde-format
msgid "MSCHAPv2"
msgstr "MSCHAPv2"

#. i18n: ectx: property (text), item, widget (QListWidget, listWidget)
